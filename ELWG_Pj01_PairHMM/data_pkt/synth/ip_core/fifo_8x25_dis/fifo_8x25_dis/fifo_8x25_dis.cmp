	component fifo_8x25_dis is
		port (
			data  : in  std_logic_vector(24 downto 0) := (others => 'X'); -- datain
			wrreq : in  std_logic                     := 'X';             -- wrreq
			rdreq : in  std_logic                     := 'X';             -- rdreq(ack)
			clock : in  std_logic                     := 'X';             -- clk
			q     : out std_logic_vector(24 downto 0);                    -- dataout
			usedw : out std_logic_vector(2 downto 0);                     -- usedw
			full  : out std_logic;                                        -- full
			empty : out std_logic                                         -- empty
		);
	end component fifo_8x25_dis;

