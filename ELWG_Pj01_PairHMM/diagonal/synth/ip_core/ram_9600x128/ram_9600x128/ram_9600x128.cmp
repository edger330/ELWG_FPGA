	component ram_9600x128 is
		port (
			data      : in  std_logic_vector(127 downto 0) := (others => 'X'); -- datain
			wraddress : in  std_logic_vector(13 downto 0)  := (others => 'X'); -- wraddress
			rdaddress : in  std_logic_vector(15 downto 0)  := (others => 'X'); -- rdaddress
			wren      : in  std_logic                      := 'X';             -- wren
			clock     : in  std_logic                      := 'X';             -- clock
			rden      : in  std_logic                      := 'X';             -- rden
			q         : out std_logic_vector(31 downto 0)                      -- dataout
		);
	end component ram_9600x128;

